{
  "services": [
    {
      "name": "pocket-id",
      "image": "ghcr.io/pocket-id/pocket-id:v1.15.0",
      "environment": {
        "TZ": "${TZ}",
        "APP_URL": "${APP_PROTOCOL:-https}://${APP_DOMAIN}",
        "TRUST_PROXY": "true"
      },
      "internalPort": 1411,
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data",
          "containerPath": "/app/data",
          "readOnly": false,
          "shared": false,
          "private": false
        }
      ],
      "isMain": true
    }
  ]
}